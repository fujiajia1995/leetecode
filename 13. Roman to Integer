class Solution(object):
    def romanToInt(self, s):
        data ={"I": 1,
               "V": 5,
               "X": 10,
               "L": 50,
               "C": 100,
               "D": 500,
               "M": 1000}
        kekka_list=[]
        for i in range(len(s)-1):
            if data[s[i]] >= data[s[i+1]]:
                kekka_list.append(data[s[i]])
            else:
                kekka_list.append(-data[s[i]])
        kekka_list.append(data[s[len(s)-1]])
        result = sum(kekka_list)
        return result
