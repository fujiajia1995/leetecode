class Solution(object):
    def combinationSum2(self, candidates, target):
        candidates =sorted(candidates)
        def same(list1, list2):
            if len(list1) != len(list2):
                return False
            for i in range(len(list1)):
                if list1[i] != list2[i]:
                    return  False
            return True

        def make_decesion(list, i):
            if list == None:
                return True
            for j in list:
                if same(j, i):
                    return False
            return True

        def make_kekka(sum, i, state, target):
            if sum == target:
                state = sorted(state)
                if make_decesion(kekka_list, state):
                   kekka_list.append(state)

            elif sum < target and i < len(candidates):
                make_kekka(sum + candidates[i], i + 1, state + [candidates[i]], target)
                make_kekka(sum, i + 1, state, target)

        if sum(candidates) < target:
            return []
        kekka_list = []
        make_kekka(0, 0, [], target)
        return kekka_list
