# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def removeNthFromEnd(self, head, n):
        """
        :type head: ListNode
        :type n: int
        :rtype: ListNode
        """
        def delete_after(node):
            if node.next.next == None:
                node.next = None
            else:
                node.next = node.next.next
        
        current = head
        
        def list_head(head):
            current = head
            n = 0
            while True:
                if current != None:
                    n += 1
                    current = current.next
                else:
                    return n
        
       
                    
                
               
        num = list_head(head)
        print(num,n,num-n)
        if num == 1 and n ==1:
            return None
        if num-n > 0:
            for i in range(num-n-1):
                current = current.next
            delete_after(current)
        else:
            return head.next
        

        return head
